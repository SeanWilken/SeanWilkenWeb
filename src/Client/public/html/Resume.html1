        :root {
            --primary: #a33123; /* deep red */
            --primary-dark: #7d261a;
            --text-main: #333333;
            --text-light: #f7f7f7;
            --border-light: #dddddd;
            --background: #ffffff;
            --sidebar-width: 260px;
        }

        * {
            box-sizing: border-box;
            margin: 0;
            padding: 0;
        }

        body {
