            font-size: 13px;
            height: 100%;
        }

        .sidebar-section {
            margin-bottom: 22px;
        }

        .sidebar-title {
            font-size: 14px;
            font-weight: 700;
            text-transform: uppercase;
            letter-spacing: 0.08em;
            margin-bottom: 8px;
            border-bottom: 1px solid rgba(255, 255, 255, 0.3);
            padding-bottom: 4px;
        }

        .sidebar p,
        .sidebar li {
            font-size: 12px;
        }

        .sidebar ul {
            list-style: none;
            margin-top: 4px;
            padding-left: 0;
        }

        .sidebar li + li {
            margin-top: 4px;
        }

        .icon-line {
            display: flex;
            align-items: baseline;
            gap: 6px;
            margin-bottom: 4px;
        }

        .icon-line span.icon {
            font-size: 12px;
            width: 14px;
            text-align: center;
        }

        a {
            color: inherit;
            text-decoration: none;
        }

        a:hover {
            text-decoration: underline;
        }

        .sidebar .bullet-list {
            margin-top: 6px;
        }

        .sidebar .bullet-list li {
            position: relative;
            padding-left: 12px;
        }

        .sidebar .bullet-list li::before {
            content: "‚Ä¢";
            position: absolute;
            left: 0;
        }

        .skills-group-title {
            font-weight: 600;
            margin-top: 6px;
        }

        /* Main column */
        .main {
            padding: 20px 30px 28px 26px;
            font-size: 13px;
        }

        .section {
            margin-bottom: 14px;
        }

        .section-title {
            font-size: 14px;
            font-weight: 700;
            text-transform: uppercase;
            color: var(--primary-dark);
            letter-spacing: 0.12em;
            border-bottom: 1px solid var(--border-light);
