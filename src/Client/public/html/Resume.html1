        }

        .job-dates {
            font-size: 12px;
            color: #555555;
        }

        .job-location {
            font-size: 12px;
            color: #555555;
        }

        .job-bullets {
            margin-top: 4px;
            padding-left: 18px;
        }

        .job-bullets li {
